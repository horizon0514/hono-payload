{
  "name": "@hono-payload/cms",
  "version": "1.0.0",
  "description": "PayloadCMS backend admin",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "biome check .",
    "format": "biome format --write .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@hono-payload/db": "workspace:*",
    "@payloadcms/db-postgres": "3.52.0",
    "@payloadcms/next": "3.52.0",
    "@payloadcms/payload-cloud": "3.52.0",
    "@payloadcms/richtext-lexical": "3.52.0",
    "@payloadcms/translations": "3.52.0",
    "@payloadcms/ui": "3.52.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "next": "15.4.4",
    "payload": "3.52.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "react": "19.1.1",
    "sharp": "0.34.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "typescript": "5.7.3"
  }
}
